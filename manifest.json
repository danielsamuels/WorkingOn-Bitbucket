{
  "manifest_version": 2,

  "name": "WorkingOn Bitbucket integration",
  "description": "This extension adds a WorkingOn button to a Bitbucket issue page.",
  "version": "1.0.1",
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["https://bitbucket.org/*/*/issues/*"],
      "js": ["scripts/jquery.js", "scripts/app.js"]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}
